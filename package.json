        "build:production": "cross-env NEXT_PUBLIC_DCORP_VERSION=$npm_package_version env-cmd -f .env.production next build",
        "lint": "npm run type:check && next lint",
        "lint:commit": "next lint",
        "lint:fix": "next lint --fix",
        "prepare": "husky install",
        "type:check": "tsc --noEmit --pretty",
        "debug-local": "cross-env NEXT_PUBLIC_DCORP_VERSION=$npm_package_version env-cmd -f .env.sample next build:develop && npm run start",
        "postinstall": "husky install"
    },
