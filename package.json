    "name": "skysurvey",
    "version": "0.2.12",
    "private": true,
    "scripts": {
        "dev": "cross-env NEXT_PUBLIC_DCORP_VERSION=$npm_package_version env-cmd -f .env.sample next dev",
        "build": "cross-env NEXT_PUBLIC_DCORP_VERSION=$npm_package_version env-cmd -f .env.sample next build",
        "start": "cross-env NEXT_PUBLIC_DCORP_VERSION=$npm_package_version next start",
        "start:staging": "cross-env NEXT_PUBLIC_DCORP_VERSION=$npm_package_version env-cmd -f .env.staging next start",
        "build:vercel": "cross-env NEXT_PUBLIC_DCORP_VERSION=$npm_package_version env-cmd -f .env.vercel next build",
        "build:develop": "cross-env NEXT_PUBLIC_DCORP_VERSION=$npm_package_version env-cmd -f .env.develop next build",
